<!doctype html>
<html>
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link href="/dist/output.css" rel="stylesheet">
  <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.4/Chart.min.js"></script>
</head>
<body>
  <div class="section_1">
    <div class="graph_result">
        <h1 class="text-3xl font-bold tracking-tight text-gray-900 sm:text-4xl">
            Result Visualization
            <%= image_tag("placeholder2.jpg", width: "1000px", height: "500px",alt:"place holder")%>
            <canvas id="myChart" width="200px" height="100px" data-labels="<%= @data_keys %>" data-data="<%= @data_values %>" ></canvas>
        
            
        </h1>

    <div class="measurements">
        <div class="forecasted-price">
          <h1 class="text-3xl font-bold tracking-tight text-gray-900 sm:text-4xl">Forecasted Price</h1>
          <div class="forecasted-price-in">
            <p> <%=@forecasted%> </p>
          </div>
          

        </div>
        <div class ="forecasted-perform">
          <h1 class="text-3xl font-bold tracking-tight text-gray-900 sm:text-4xl">performance</h1>

        </div>

    </div>
  </div>
</body>
</html>