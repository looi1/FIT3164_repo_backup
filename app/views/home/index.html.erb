  </div>
  </div>
  </div>

    <div class ="section_all section_2">
      <div class="measurements">
      <div class="forecasted-price">
        <h1 class="text-3xl font-bold tracking-tight text-gray-900 sm:text-4xl">Mean Absolute Error</h1>
        <div class="forecasted-price-in">
         
          <p id="forecast"></p>
          <h1 id="x"><%= @para %></h1> 
        </div>
        

      </div>
      
      <div class ="forecasted-perform">
        <h1 class="text-3xl font-bold tracking-tight text-gray-900 sm:text-4xl">performance</h1>
        <p id="perform">100</p>

      </div>

      </div>



    </div>
</div>


<%= javascript_include_tag('chart_result.js') %>
<%= javascript_include_tag('dropdown.js') %>
