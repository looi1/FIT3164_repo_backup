<!doctype html>
<html>
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link href="/dist/output.css" rel="stylesheet">
  <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.4/Chart.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/PapaParse/5.3.0/papaparse.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/chartjs-plugin-annotation/0.5.7/chartjs-plugin-annotation.min.js"></script>

  
</head>
<body onload="myFunction()">
  <div class = "page">
    <div class="section_all section_1">
      <h1 id = "vis-title" class=" text-3xl font-bold tracking-tight text-gray-900 sm:text-4xl" ></h1>
      <div class="graph_result">
          <div class="graph_result_1">
            <div class="graph_result_2">
             
              
              <div id= "chart-container-1" >
                <canvas id="myChart"  style="width: 100%; height: 500px;" data-labels="<%= @data_keys %>" data-data="<%= @data_values %>" ></canvas>
              </div>

              <div id= "chart-container-2" >
                <canvas id="myChart2"  style="width: 100%; height: 500px;"  data-labels="<%= @data_keys %>" data-data="<%= @data_values %>" ></canvas>
              </div>

              <div id= "chart-container-3" >
                <canvas id="myChart3"  style="width: 100%; height: 500px;" data-labels="<%= @data_keys %>" data-data="<%= @data_values %>" ></canvas>
              </div>

              <div id= "chart-container-4" >
                <canvas id="myChart4"  style="width: 100%; height: 500px;" data-labels="<%= @data_keys %>" data-data="<%= @data_values %>" ></canvas>
              </div>

              <div id= "chart-container-5" >
                <canvas id="myChart5"  style="width: 100%; height: 500px;" data-labels="<%= @data_keys %>" data-data="<%= @data_values %>" ></canvas>
              </div>

            </div>
      <div class= "sec-2">
        <div class= "sec-2-title">
          <h1 class=" text-3xl font-bold tracking-tight text-gray-900 sm:text-4xl">Prediction Summary</h1>
        </div>
          <div class="left-side">
            <div class = "slope-chart">
            
              <div id ="chart-container-6">
                <canvas id="myChart6"  style="width: 500px; height: 250px;" data-labels="<%= @data_keys %>" data-data="<%= @data_values %>" ></canvas>
              </div>
              <div id ="chart-container-7">
                <canvas id="myChart7"  style="width: 500px; height: 250px;" data-labels="<%= @data_keys %>" data-data="<%= @data_values %>" ></canvas>
              </div>
              <div id ="chart-container-8">
                <canvas id="myChart8"  style="width: 500px; height: 250px;" data-labels="<%= @data_keys %>" data-data="<%= @data_values %>" ></canvas>
              </div>
              <div id ="chart-container-9">
                <canvas id="myChart9"  style="width: 500px; height: 250px;" data-labels="<%= @data_keys %>" data-data="<%= @data_values %>" ></canvas>
              </div>
              <div id ="chart-container-10">
                <canvas id="myChart10"  style="width: 500px; height: 250px;" data-labels="<%= @data_keys %>" data-data="<%= @data_values %>" ></canvas>
              </div>

              

            </div>

            <div class="slope-performance">
              <div class = "slope-price" style="display: flex; flex-direction: column; gap: 1rem; width: 70%;">
                <div class="slope-s-price" style="display: flex; justify-content: space-between; background-color: lightgray; border-radius: 1rem; padding: 1rem;">
                  <p id="perform-start"></p>
                  <p id="perform-start-price"></p>
                </div>
                <div class="slope-e-price" style="display: flex; justify-content: space-between; background-color: lightgray; border-radius: 1rem; padding: 1rem;">
                  <p id="perform-end"></p>
              
                  <p id="perform-end-price"></p>
                </div>

                <div class="slope-p-price" style="width: 70%;">
              </div>
            
            
            
            <p id="perform-percent" style="text-align: center;"></p>
            </div>

          </div>

          </div>

        </div>
      </div>
  </div>
  </div>
  </div>

    
</div>


<%= javascript_include_tag('chart_result.js') %>
<%= javascript_include_tag('dropdown.js') %>
</body>
</html>



